{
    "name": "erikwegner/contao-color-box-bundle",
    "type": "contao-bundle",
    "description": "Color box extension for contao",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Erik Wegner",
            "homepage": "https://github.com/erikwegner"
        }
    ],
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "4.6.*",
        "symfony/framework-bundle": "3.4.* || 4.1.*"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.3.1",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^7.0",
        "symfony/phpunit-bridge": "^3.4.5"        
    },
    "extra": {
        "contao-manager-plugin": "Contao\\ContaoColorBoxBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Contao\\ContaoColorBoxBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contao\\ContaoColorBoxBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/erikwegner/contao-color-box-bundle/issues",
        "source": "https://github.com/erikwegner/contao-color-box-bundle"
    }
}
